{
  "name":"ebhuvon",
    "type": "mongodb",
    "host": "localhost",
    "port": 27017,
    "database": "ebhuvon",
    "synchronize": false,
    "useNewUrlParser": true,
    "logging": true,
    "useUnifiedTopology": true,
  "entities": [
    "src/entity/*.js"
  ],
  "subscribers": [
    "src/subscriber/*.js"
  ],
  "migrations": [
    "src/migration/*.js"
  ],
  "cli": {
    "entitiesDir": "src/entity",
    "migrationsDir": "src/migration",
    "subscribersDir": "src/subscriber"
  }
}